def divide_numbers(a, b):
    try:
        result = a / b
    except ZeroDivisionError:
        return "Error: Division by zero is not allowed."
    else:
        return f"The result is {result}"
        
numerator = 10
denominator = 0
print(divide_numbers(numerator, denominator))


